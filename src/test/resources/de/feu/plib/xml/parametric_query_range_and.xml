<?xml version="1.0" encoding="UTF-8"?>
<qy:query xsi:schemaLocation="urn:iso:std:iso:ts:29002:-31:ed-1:tech:xml-schema:query query.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cat="urn:iso:std:iso:ts:29002:-10:ed-1:tech:xml-schema:catalogue" xmlns:val="urn:iso:std:iso:ts:29002:-10:ed-1:tech:xml-schema:value" xmlns:qy="urn:iso:std:iso:ts:29002:-31:ed-1:tech:xml-schema:query" xmlns:bas="urn:iso:std:iso:ts:29002:-4:ed-1:tech:xml-schema:basic">
    <!-- Query for all Items belonging to Class with IRDI 0173-1#01-BAD803#2 "Skalpell" where the property
     0173-1#02-AAA762#1 - KlingenlÃ¤nge is greater then min_value and the property "Schneidbreite" is smaller than 15.0
     We have a match.
      -->
    <qy:class_ref>0173-1#01-BAD803#2</qy:class_ref>
    <qy:characteristic_data_query_expression>
        <qy:and>
            <qy:operand>
                <qy:range>
                    <qy:property_reference property_ref="0173-1#02-AAA762#1"/>
                    <qy:min_value>80.0</qy:min_value>
                    <qy:is_inclusive>true</qy:is_inclusive>
                </qy:range>
            </qy:operand>
            <qy:operand>
                <qy:range>
                    <qy:property_reference property_ref="0173-1#02-AAB011#1"/>
                    <qy:max_value>15.0</qy:max_value>
                    <qy:is_inclusive>true</qy:is_inclusive>
                </qy:range>
            </qy:operand>
        </qy:and>
    </qy:characteristic_data_query_expression>

</qy:query>

